{
  "name": "vue-ultimate-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": ["eslint --fix", "prettier --write"],
    "*.{css,scss}": ["prettier --write"]
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "tauri": "tauri",
    "prepare": "husky",
    "commit": "cz",
    "commit:quick": "git commit -m",
    "commit-and-push": "bun run commit && git push origin $(git branch --show-current)"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@formkit/auto-animate": "^0.8.2",
    "@internationalized/date": "^3.8.2",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-query": "^5.85.5",
    "@tanstack/vue-table": "^8.21.3",
    "@rollup/plugin-yaml": "^4.1.2",
    "@unovis/ts": "^1.5.2",
    "@unovis/vue": "^1.5.2",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.7.0",
    "@vueuse/integrations": "^13.7.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-vue": "^8.6.0",
    "lucide-vue-next": "0.540.0",
    "motion-v": "1.7.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "reka-ui": "^2.4.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.7",
    "universal-cookie": "^8.0.1",
    "vaul-vue": "^0.4.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.18",
    "vue-chrts": "^0.2.3",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.8",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@faker-js/faker": "^9.9.0",
    "@iconify-json/simple-icons": "^1.2.48",
    "@iconify/vue": "^5.0.0",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@playwright/test": "^1.53.1",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/vue-query-devtools": "^5.85.5",
    "@tauri-apps/cli": "^2",
    "@tsconfig/node22": "^22.0.2",
    "@types/axios": "^0.14.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.33.0",
    "eslint-plugin-oxlint": "~1.1.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "markdown-it-prism": "^3.0.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.1.0",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^29.0.0",
    "unplugin-vue-markdown": "^29.1.0",
    "unplugin-vue-router": "^0.15.0",
    "vite": "^7.1.3",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
