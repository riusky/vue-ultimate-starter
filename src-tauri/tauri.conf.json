{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vue-ultimate-starter",
  "version": "0.1.0",
  "identifier": "com.riudu",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build-only",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "vue-ultimate-starter",
        "width": 1200,
        "height": 800,
        "center": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
